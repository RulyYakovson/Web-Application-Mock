<div class="jumbotron text-center">
        <h1>Employees</h1>
</div>
<div style="margin: 0px 130px 50px 130px">
    <table class="table">
        <tbody>
            <% for(let i = 0; i < employees.length; i++) { %>
                <% if(i % 5 === 0) { %>
                    <tr>
                <% } %>
                <td>
                    <div class="user-td-style">
                        <% include ../partials/employee %>
                    </div>
                </td>
                <% if(i % 5 === 4) { %>
                    </tr>
                <% } %>
            <% } %>
        </tbody>
    </table>
</div>